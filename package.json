{
  "name": "sinergicon-landing",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "build:dev": "tsc --noEmit && vite build --mode development",
    "preview": "vite preview",
    "lint:fix": "cross-env eslint --fix 'src/**/*.{ts,js,jsx,tsx}'",
    "lint:check": "cross-env eslint 'src/**/*.{ts,js,jsx,tsx}'",
    "typecheck": "tsc --noEmit",
    "check": "npm run lint:check && npm run typecheck"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.3",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^8.32.0"
  }
}
